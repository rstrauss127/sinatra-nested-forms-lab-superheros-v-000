
<% team = @params %>
<h1><%= team[:name] %> </h1>
<h2>Team Motto <%= team[:motto] %></h2>

<div class="member1">
  <h2>Hero Name: <%= team[:member][0][:name] %></h2>
  <p>Hero Power: <%= team[:member][0][:power] %></p>
  <p>Hero Biography: <%= team[:member][0][:bio] %></p>
</div>

<div class="member2">
  <h2>Hero Name: <%= team[:member][1][:name] %></h2>
  <p>Hero Power: <%= team[:member][1][:power] %></p>
  <p>Hero Biography: <%= team[:member][1][:bio] %></p>
</div>

<div class="member3">
  <h2>Hero Name: <%= team[:member][2][:name] %></h2>
  <p>Hero Power: <%= team[:member][2][:power] %></p>
  <p>Hero Biography: <%= team[:member][2][:bio] %></p>
</div>