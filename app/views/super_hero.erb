<h1>Create a Team and Heroes!</h1>\n

<form action="/teams" method="POST">
  <label for="team_name">Team Name</label>
  <input type="text" name="team[name]" id="team_name">\n

  <label for="team_motto">Team Motto</label>
  <input type="text" name="team[motto]" id="team_motto">

  <label for="member1_name">Member 1 Name</label>
  <input type="text" name="team[member][][name]" id="member1_name">

  <label for="member1_power">Member 1 Power</label>
  <input type="text" name="team[member][][power]" id="member1_power">

  <label for="member1_bio">Member 1 Bio</label>
  <input type="text" name="team[member][][bio]" id="member1_bio">

  <label for="member2_name">Member 2 Name</label>
  <input type="text" name="team[member][][name]" id="member2_name">

  <label for="member2_power">Member 2 Power</label>
  <input type="text" name="team[member][][power]" id="member2_power">

  <label for="member2_bio">Member 2 Bio</label>
  <input type="text" name="team[member][][bio]" id="member2_bio">

  <label for="member3_name">Member 3 Name</label>
  <input type="text" name="team[member][][name]" id="member3_name">

  <label for="member3_power">Member 3 Power</label>
  <input type="text" name="team[member][][power]" id="member3_power">

  <label for="member3_bio">Member 3 Bio</label>
  <input type="text" name="team[member][][bio]" id="member3_bio">

  <input type="submit" id="Submit">
</form>
